
<table class=trip-table id=search-results>
  <thead>
  <tr>
    <th>Name
    <th>Email
    <th>Profile
  </tr>
  </thead>

  <tbody>
  {% for user in users %}
  <tr>
    <td>{{user.name}}
    <td>{{user.email}}</td>
    <td><a href="/profile/{{user.id}}">Profile</a></td>
  </tr>
  {% endfor %}
  </tbody>
</table>
